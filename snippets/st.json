//conditions statements

	"IF ...THEN ... END_IF": {
		"prefix": "if,then,end_if",
		"body": [
			"IF ${1:Condition} THEN" ,
			"\t$0",
			"END_IF;"
		],
		"description": "IF ...THEN ... END_IF"
		},


	"IF ...ELSE": {
		"prefix": "if,else,then,end_if",
		"body": [
			"IF ${1:Condition} THEN" ,
			"\t$0",
			"ELSE",
			"\t$0",
			"END_IF;"
		],
		"description": "IF ...ELSE"
		},

		"IF ...ELSIF": {
		"prefix": "elsif",
		"body": [
			"IF ${1:Condition} THEN" ,
			"\t$0",
			"ELSIF",
			"\t$0",
			"END_IF;"
		],
		"description": "IF ... ELSIF"
		},
	"CASE ...ELSE ...END_CASE": {
		"prefix": "case",
		"body": [
			"CASE ${1:Variable} OF ${2:Value}:" ,
			"\t$0",
			"ELSE",
			"\t",
			"END_CASE;"
		],
		"description": "CASE ...ELSE ...END_CASE;"
		},

//END CONDITION statements



//LOOP statements
	"WHILE ...DO ... END_WHILE": {
		"prefix": "while",
		"body": [
			"WHILE ${1:Boolean}" ,
			"\t$0",
			"DO",
			"\t$0",
			"END_WHILE;"
		],
		"description": "WHILE ...DO ... END_WHILE"
		},
	
	"REPEAT ...UNTIL ... END_REPEAT": {
		"prefix": "repeat",
		"body": [
			"REPEAT" ,
			"\t$0",
			"UNTIL ${1:Condition}",
			"\t$0",
			"END_REPEAT;"
		],
		"description": "REPEAT ...UNTIL ... END_REPEAT"
		},

	"FOR ...DO ... END_FOR": {
		"prefix": "for",
		"body": [
			"FOR ${1:Condition}" ,
			"\t$0",
			"TO ${2:value}",
			"\t$0",
			"BY ${3:value}",
			"\t$0",
			"DO",
			"\t$0",
			"END_FOR;"
		],
		"description": "FOR ...DO ... END_FOR"
		}
//end LOOP statements



