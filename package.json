{
	"name": "StructuredText",
	"displayName": "Structured Text PLCs",
	"version": "0.1.1",
	"publisher": "SmookCreative",
	"icon": "Img/StructuredText.png",
	"engines": { "vscode": "*" },
	"description": "Structured Text is a language support for programmable logic controllers (PLCs)",
	"author": {
        "name": "Smook Creative"
	},


	
	"keywords": [
		"structured",
		"text",
		"PLCS"
	  ],

	"readme": "README.md",
	

	"categories": [
        "Languages"
    ],
	
	"scripts": {
		"update-grammar": "node ../../build/npm/update-grammar.js textmate/st.tmbundle ./syntaxes/st.tmlanguage.json"
	},
	"contributes": {
		"languages": [{
			"id": "st",
			"extensions": [ ".st" ],
			"aliases": [ "Structured Text", "st" ],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "st",
			"scopeName": "structured text",
			"path": "./syntaxes/st.tmlanguage.json"
		}],
		"snippets": [{
			"language": "st",
			"path": "./snippets/st.json"
		}]
	}
}
